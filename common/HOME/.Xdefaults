Xft.dpi: 96
! Xft.dpi: 236
URxvt.termName:        xterm-256color
URxvt.scrollBar:       false
URxvt.saveLines:       0
URxvt.visualBell:      false
URxvt.iso14755:        false
URxvt.iso14755_52:     false
URxvt.perl-ext-common: default,matcher,font-size
URxvt.url-launcher:    /bin/firefox
URxvt.matcher.button:  1
URxvt.matcher.rend.0:  Uline Bold fg7
URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show
URxvt.xftAntialias:    true
URxvt.depth:           32
URxvt.letterSpace:     0

#if defined(HOME)
URxvt.font: xft:DejaVu Sans Mono:style=Regular:pixelsize=14,\
            xft:Noto Sans CJK JP:style=Regular:pixelsize=14,\
            xft:Noto Sans Mono:style=Regular:pixelsize=14,\
            xft:symbola:style=Regular:pixelsize=10:minspace=False
#elif defined(LEGION)
URxvt.font: xft:DejaVu Sans Mono:style=Regular:pixelsize=20,\
            xft:Noto Sans CJK JP:style=Regular:pixelsize=20,\
            xft:Noto Sans Mono:style=Regular:pixelsize=20,\
            xft:symbola:style=Regular:pixelsize=20:minspace=False
#else
#error "unknown machine"
#endif

URxvt.background:     [60]#000000
URxvt.foreground:         #FFFFFF
URxvt.color0:             #3f3f3f
URxvt.color1:             #cc9393
URxvt.color2:             #7f9f7f
URxvt.color3:             #d0bf8f
URxvt.color4:             #6ca0a3
URxvt.color5:             #dc8cc3
URxvt.color6:             #93e0e3
URxvt.color7:             #dcdccc
URxvt.color8:             #000000
URxvt.color9:             #dca3a3
URxvt.color10:            #bfebbf
URxvt.color11:            #f0dfaf
URxvt.color12:            #8cd0d3
URxvt.color13:            #dc8cc3
URxvt.color14:            #93e0e3
URxvt.color15:            #3F3F6A
URxvt.cursorColor:        #aaaaaa
URxvt.colorUL:            #366060
URxvt.underlineColor:     #dfaf8f
URxvt.highlightColor:     #202020
URxvt.highlightTextColor: #FFFFFF
!URxvt.colorBD:           #
!URxvt.colorIT:           #
